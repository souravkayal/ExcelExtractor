<script type="text/javascript"
        src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js">
</script>
<script src="jquery.plug-in.js" type="text/javascript"></script>
<script src="custom.js" type="text/javascript"></script>
<div class="jumbotron">
    <h1>ASP.NET</h1>

</div>
<div class="row">

    <script type="text/javascript" language="javascript">
        $(document).ready(function () {
            $('#btnSubmit').click(function () {
                if ($('#uploadfile').val() == '') {
                    alert('Please select file');
                    return;
                }
                var formData = new FormData();
                var file = $('#uploadfile')[0];
                formData.append('file', file.files[0]);
                $.ajax({
                    url: 'https://localhost:44303/api/Excel',
                    type: 'POST',
                    data: formData,
                    contentType: false,
                    processData: false,
                    success: function (result) {
                        
                        $('#CustomerName').text(result.CustomerName);
                        $('#CustomerId').text(result.CustomerId);
                        $('#CustomerSalary').text(result.CustomerSalary);
                        $('#CustomerDes').text(result.CustomerDesignation);

                    },
                    error: function (error) {
                        alert("Faild please try upload again" + JSON.stringify(error));
                    }
                });
            });
        });
    </script>

    <div class="panel-body">
        <div class="form-group"> <input type="file" id="uploadfile" /> </div> <input id="btnSubmit" class="btn btn-default" type="button" value="Upload" />
        <div class="text-info" id="status"></div>
    </div>

    <div  class="panel-body">
        <div>Customer Name : <span id="CustomerName"></span> </div>
        <div>Customer Id : <span id="CustomerId"></span> </div>
        <div>Customer Salary : <span id="CustomerSalary"></span> </div>
        <div>Customer Designation : <span id="CustomerDes"></span> </div>
    </div>

</div>